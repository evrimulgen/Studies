package td1;

public class ListeChainee {
	private Node racine;
	private class Node {
		private String nom;
		private Node succ;
		
		public Node(String n){
			nom=n;
			succ=null;
		}
		
	}
	
	public void ajouterNoeud(String n){
		Node tete = new Node(n);
		tete.succ=racine;
		racine = tete;
	}
	
	

}
