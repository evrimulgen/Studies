DROP TABLE OPERATEUR
DROP TABLE MACHINE 
DROP TABLE PIECE
DROP TABLE QUALIFIE_SUR


CREATE TABLE OPERATEUR(
NUMERO VARCHAR (20)PRIMARY KEY,
NOM   CHARACTER(20) NOT NULL,
AGE   NUMERIC(2) );

CREATE TABLE MACHINE(
REFERENCE_M VARCHAR(20) PRIMARY KEY,
DESIGNATION_M CHARACTER(20) NOT NULL)

CREATE TABLE PIECE(
REFERENCE_P VARCHAR(20) PRIMARY KEY,
DESIGNATION_P CHARACTER(20) NOT NULL,
QUANTITE NUMERIC(5),
REFERENCE_MACHINE NUMERIC(20),
NUMERO_OPERATEUR NUMERIC(20),
CONSTRAINT P_M FOREIGN KEY (REFERNCE_MACHINE) REFERS (MACHINE),
CONSTRAINT P_O FOREIGN KEY (NUMERO_OPERATEUR) REFERS (OPERATEUR)
)

CREATE TABLE QUALIFIE_SUR(
PRIMARY KEY(NUMERO_OPERATEUR,REFERENCE_MACHINE),
DT DATE )
